                                        <mo class="MathClass-open" stretchy="false">{</mo>
                                        <mn>1</mn>
                                        <mo class="MathClass-close" stretchy="false">}</mo>
                                    </mrow>
                                </math>
                            </td>
                            <td class="td11" id="TBL-5-7-2" style="text-align:center; white-space:nowrap;"> <br> </td>
                            <td class="td11" id="TBL-5-7-3" style="text-align:center; white-space:nowrap;"> 
